#!/bin/bash

IP="74.110.137.121"
PORT="8080"
LEAGUE=$1
TIER=$2
SEASON=$3
ROUND=$4
HOME=$5
AWAY=$6
SCORE=$7

URL="http://${IP}:${PORT}"
API="${URL}/api"

curl -X PUT \
-H "Content-Type: application/json" \
-d '{"round":'${ROUND}',"score":'${SCORE}'}' \
"${API}/leagues/${LEAGUE}/${TIER}/${SEASON}/results/${HOME}/${AWAY}"
